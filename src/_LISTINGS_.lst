00003A04 321B                       		move.w	(a3)+,d1			; Get starting X position
00003A06 3141 0014                  		move.w	d1,oXPos(a0)			; Set the player's X position
00003A0A 3013                       		move.w	(a3),d0				; Get starting Y position
