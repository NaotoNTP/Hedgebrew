000039EE 42B8 F8C8                  		clr.l	rMinCam.w			; Set minimum camera values
000039F2 21D3 F8BC                  		move.l	(a3),rDestMaxCam.w		; Set target maximum camera values
000039F6 21DB F8C0                  		move.l	(a3)+,rMaxCam.w		; Set maximum camera values
000039FA                            
